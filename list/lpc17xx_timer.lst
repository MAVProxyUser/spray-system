Nextern uint32_t idx_7,idx_6;
Nextern uint16_t idx_7_freq,idx_6_freq;
Nstatic volatile uint8_t timer0_1ms_timeout_flag = 0;
Nextern volatile uint32_t g_current_tick ;
Nuint32_t idx_7_tick,idx_6_tick;
Nvoid TIMER0_IRQHandler( void )
N{
N	uint32_t ir_reg = LPC_TIM0->IR;
X	uint32_t ir_reg = ((LPC_TIM_TypeDef *) ((0x40000000UL) + 0x04000) )->IR;
N	LPC_TIM0->IR = ir_reg;
X	((LPC_TIM_TypeDef *) ((0x40000000UL) + 0x04000) )->IR = ir_reg;
N	if(g_current_tick % 1000 == 0)
N	{
N		idx_7_freq = idx_7;
N		idx_6_freq = idx_6;
N		idx_6 = 0;
N		idx_7 = 0;
N	}
N	
N	if(FOC_1_STATUS.enable_flag == STOP_SPARY&&
X	if(FOC_1_STATUS.enable_flag == 0&&
N  	 FOC_2_STATUS.enable_flag == STOP_SPARY)
X  	 FOC_2_STATUS.enable_flag == 0)
N	{
N
N  }
N	else if(FOC_1_STATUS.flow_speed == 0
N		   && FOC_2_STATUS.flow_speed == 0)
N	{
N	
N	}	
N	else
N	{
N		if(idx_7_freq > 5) //  
N			idx_7_tick++;
N		
N		if(idx_6_freq > 5) //  
N			idx_6_tick++;
N	}
N	timer0_1ms_timeout_flag = 1;
N	g_current_tick ++ ;
N}
N
Nvoid TIMER0_Init( void )
N{
N	LPC_SC->PCONP |= ( 1 << 1 );
X	((LPC_SC_TypeDef *) ((0x40080000UL) + 0x7C000) )->PCONP |= ( 1 << 1 );
N	
N	LPC_TIM0->TCR = 0x02;
X	((LPC_TIM_TypeDef *) ((0x40000000UL) + 0x04000) )->TCR = 0x02;
N	LPC_TIM0->IR   = 0xF;
X	((LPC_TIM_TypeDef *) ((0x40000000UL) + 0x04000) )->IR   = 0xF;
N	LPC_TIM0->CTCR = 0;
X	((LPC_TIM_TypeDef *) ((0x40000000UL) + 0x04000) )->CTCR = 0;
N	LPC_TIM0->PR   = 24;
X	((LPC_TIM_TypeDef *) ((0x40000000UL) + 0x04000) )->PR   = 24;
N	LPC_TIM0->MR0	 = 1000; 	//1ms
X	((LPC_TIM_TypeDef *) ((0x40000000UL) + 0x04000) )->MR0	 = 1000; 	
N	LPC_TIM0->MCR  = 0x3;
X	((LPC_TIM_TypeDef *) ((0x40000000UL) + 0x04000) )->MCR  = 0x3;
N	NVIC_EnableIRQ( TIMER0_IRQn );
N}
N
Nvoid TIMER0_Start( void )
N{
N	LPC_TIM0->TCR = 0x01;
X	((LPC_TIM_TypeDef *) ((0x40000000UL) + 0x04000) )->TCR = 0x01;
N}
N
Nvoid TIMER0_Stop( void )
N{
N	LPC_TIM0->TCR &= ~0x01;
X	((LPC_TIM_TypeDef *) ((0x40000000UL) + 0x04000) )->TCR &= ~0x01;
N}
N
Nvoid TIMER0_Reset( void )
N{
N	LPC_TIM0->TCR |= 0x02;
X	((LPC_TIM_TypeDef *) ((0x40000000UL) + 0x04000) )->TCR |= 0x02;
N}
N
Nuint8_t TIMER0_1Ms_Timeout( void )
N{
N	if( timer0_1ms_timeout_flag ) {
N		timer0_1ms_timeout_flag = 0;
N		return 1;
N	} else {
N		return 0;
N	}
N}
