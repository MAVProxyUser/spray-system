Nvoid TIMER2_Init( void )
N{
N	LPC_SC->PCONP |= ( 1 << 22 );
X	((LPC_SC_TypeDef *) ((0x40080000UL) + 0x7C000) )->PCONP |= ( 1 << 22 );
N	LPC_TIM2->TCR = 0x02;
X	((LPC_TIM_TypeDef *) ((0x40080000UL) + 0x10000) )->TCR = 0x02;
N	LPC_TIM2->IR   = 0xF;
X	((LPC_TIM_TypeDef *) ((0x40080000UL) + 0x10000) )->IR   = 0xF;
N	LPC_TIM2->CTCR = 0;
X	((LPC_TIM_TypeDef *) ((0x40080000UL) + 0x10000) )->CTCR = 0;
N	LPC_TIM2->PR   = 24;
X	((LPC_TIM_TypeDef *) ((0x40080000UL) + 0x10000) )->PR   = 24;
N	LPC_TIM2->MR0	 = 300; 	// 0.1ms
X	((LPC_TIM_TypeDef *) ((0x40080000UL) + 0x10000) )->MR0	 = 300; 	
N	LPC_TIM2->MCR  = 0x3;
X	((LPC_TIM_TypeDef *) ((0x40080000UL) + 0x10000) )->MCR  = 0x3;
N	NVIC_EnableIRQ( TIMER2_IRQn );
N}
N
N
Nvoid TIMER2_Start( void )
N{
N	LPC_TIM2->TCR = 0x01;
X	((LPC_TIM_TypeDef *) ((0x40080000UL) + 0x10000) )->TCR = 0x01;
N}
N
Nvoid TIMER2_Stop( void )
N{
N	LPC_TIM2->TCR &= ~0x01;
X	((LPC_TIM_TypeDef *) ((0x40080000UL) + 0x10000) )->TCR &= ~0x01;
N}
N
N
Nvoid TIMER2_IRQHandler( void )
N{
N	uint32_t ir_reg = LPC_TIM2->IR;
X	uint32_t ir_reg = ((LPC_TIM_TypeDef *) ((0x40080000UL) + 0x10000) )->IR;
N	LPC_TIM2->IR = ir_reg;
X	((LPC_TIM_TypeDef *) ((0x40080000UL) + 0x10000) )->IR = ir_reg;
N	TIMER2_Stop();
N	Radar_RS485_RD();
X	((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00020) )->FIOCLR|=1<< 0;
N}
